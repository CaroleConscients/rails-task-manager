<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :priority %>
  <%= f.number_field :priority %>

  <%= f.label :dead_line %>
  <%= f.date_field :dead_line %>

  <%= f.submit %>
  <% end %>

